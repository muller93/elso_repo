a = 22
b = 12
c = 21
    switch (a) {
        case c:
        console.log(c, ' a szám');
        break
        case b:
        console.log(b, ' a szám');
        default:
        console.log('ez a szám', a);
    }

//function

function megertem(j, k){
    return j * k ;
}
console.log(megertem(3, 5));